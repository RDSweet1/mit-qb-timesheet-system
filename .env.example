# QuickBooks OAuth Credentials
# Get these from: https://developer.intuit.com → Dashboard → Your App → Keys & OAuth
QB_CLIENT_ID=your_client_id_here
QB_CLIENT_SECRET=your_client_secret_here

# QuickBooks Tokens (from OAuth Playground)
# These go in Azure Key Vault for production, but can use locally for testing
QB_ACCESS_TOKEN=your_access_token_here
QB_REFRESH_TOKEN=your_refresh_token_here
QB_REALM_ID=your_realm_id_here

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Azure (for production - Key Vault storage)
AZURE_TENANT_ID=your_tenant_id
AZURE_KV_CLIENT_ID=your_keyvault_client_id
AZURE_KV_CLIENT_SECRET=your_keyvault_client_secret
AZURE_VAULT_NAME=your_vault_name

# Email (Resend)
RESEND_API_KEY=your_resend_api_key
FROM_EMAIL=timesheets@yourdomain.com

# Environment
QB_ENVIRONMENT=sandbox
# Change to 'production' when ready:
# QB_ENVIRONMENT=production
